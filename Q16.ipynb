   "source": [
    "import numpy as np\n",
    "def convert(li):\n",
    "    return np.array(li)\n",
    "li=[]\n",
    "n=int(input(\"enter the number of elements\"))\n",
    "for i in range(n):\n",
    "    x=int(input())\n",
    "    li.append(x)\n",
    "convert(li)"
   ]
