    "\"\"\" Create a NumPy function that extracts elements \n",
    "from a 2D array using row and column indices provided \n",
    "in separate arrays\"\"\"\n",
    "import numpy as np\n",
    "def ele(row,col):\n",
    "    return arr[row,col]\n",
    "arr=np.arange(12).reshape(4,3)\n",
    "print(arr)\n",
    "row=np.array([1,2])\n",
    "col=np.array([0,1])\n",
    "ele(row,col)"
